<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_github"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.restaurantreview.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/detailUser"
        android:name="com.example.restaurantreview.ui.DetailUser"
        android:label="activity_detail_user"
        tools:layout="@layout/activity_detail_user" >
        <argument
            android:name="avatar"
            app:argType="string" />
        <argument
            android:name="username"
            app:argType="string" />
        <argument android:name="display_name" />
        <argument android:name="follow_stats" />
    </activity>
</navigation>